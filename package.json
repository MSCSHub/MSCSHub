{
  "name": "utmscs",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start-msds": "ng serve --configuration=msds",
    "start-msai": "ng serve --configuration=msai",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "compodoc": "npx compodoc -p tsconfig.doc.json",
    "ng-deploy": "ng deploy",
    "deploy": "npm run build && npm run deploy-both && npm run doc",
    "doc": "(npm run compodoc) && (cd documentation && git add . && git diff-index --quiet HEAD || (git commit -a -m 'Updated' && git push))",
    "deploy-mscs": "ng build --configuration=mscs && ng deploy --base-href=https://mscshub.com --cname=mscshub.com --repo=git@github.com:MSCSHub/MSCSHub.git --no-build --no-silent",
    "deploy-msds": "ng build --configuration=msds && ng deploy --base-href=https://msdshub.com --cname=msdshub.com --repo=git@github.com:MSCSHub/MSDSHub.git --no-build --no-silent",
    "deploy-msai": "ng build --configuration=msai && ng deploy --base-href=https://msaihub.com --cname=msaihub.com --repo=git@github.com:MSCSHub/MSAIHub.git --no-build --no-silent",
    "deploy-both": "npm run deploy-mscs && npm run deploy-msds"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^18.1.3",
    "@angular/cdk": "^18.1.3",
    "@angular/common": "^18.1.3",
    "@angular/compiler": "^18.1.3",
    "@angular/core": "^18.1.3",
    "@angular/fire": "^18.0.1",
    "@angular/flex-layout": "^15.0.0-beta.35",
    "@angular/forms": "^18.1.3",
    "@angular/material": "^18.1.3",
    "@angular/platform-browser": "^18.1.3",
    "@angular/platform-browser-dynamic": "^18.1.3",
    "@angular/router": "^18.1.3",
    "ansi-regex": ">=5.0.1",
    "firebase": "^10.12.5",
    "glob-parent": ">=5.1.2",
    "path-parse": ">=1.0.7",
    "rxfire": "^6.0.5",
    "rxjs": "^7.4.0",
    "tslib": "^2.6.3",
    "zone.js": "~0.14"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^18.1.3",
    "@angular/cli": "^18.1.3",
    "@angular/compiler-cli": "^18.1.3",
    "@types/jasmine": "~3.8.0",
    "@types/node": "^12.20.55",
    "angular-cli-ghpages": "^2.0.1",
    "jasmine-core": "~3.8.0",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "^3.2.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "^4.0.2",
    "karma-jasmine-html-reporter": "~1.7.0",
    "typescript": "~5.5.4"
  },
  "overrides": {
    "gh-pages": "^6.1.1"
  }
}
