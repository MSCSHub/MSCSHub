{
  "name": "utmscs",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "compodoc": "npx compodoc -p tsconfig.doc.json",
    "ng-deploy": "ng deploy",
    "deploy": "npm run build && npm run deploy-both && npm run doc",
    "doc": "(npm run compodoc) && (cd documentation && git add . && git diff-index --quiet HEAD || (git commit -a -m 'Updated' && git push))",
    "deploy-mscs": "ng build --configuration=mscs && ng deploy --base-href=https://mscshub.com --cname=mscshub.com --repo=git@github.com:MSCSHub/MSCSHub.git --no-build",
    "deploy-msds": "ng build --configuration=msds && ng deploy --base-href=https://msdshub.com --cname=msdshub.com --repo=git@github.com:MSCSHub/MSDSHub.git --no-build",
    "deploy-both": "npm run deploy-mscs && npm run deploy-msds"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^11.2.13",
    "@angular/cdk": "^11.2.12",
    "@angular/common": "^11.2.13",
    "@angular/compiler": "^11.2.13",
    "@angular/core": "^11.2.13",
    "@angular/fire": "^6.1.4",
    "@angular/flex-layout": "^11.0.0-beta.33",
    "@angular/forms": "^11.2.13",
    "@angular/material": "^11.2.12",
    "@angular/platform-browser": "^11.2.13",
    "@angular/platform-browser-dynamic": "^11.2.13",
    "@angular/router": "^11.2.13",
    "firebase": "^8.5.0",
    "rxjs": "~6.6.7",
    "tslib": "^2.2.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/architect": "0.1101.1",
    "@angular-devkit/build-angular": "^0.1102.12",
    "@angular/cli": "^11.2.12",
    "@angular/compiler-cli": "^11.2.13",
    "@types/jasmine": "^3.6.11",
    "@types/node": "^12.20.12",
    "angular-cli-ghpages": "^1.0.0-rc.1",
    "codelyzer": "^6.0.2",
    "firebase-tools": "^9.10.2",
    "fuzzy": "^0.1.3",
    "inquirer": "^6.2.2",
    "inquirer-autocomplete-prompt": "^1.0.1",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "^6.3.2",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "open": "^7.0.3",
    "protractor": "~7.0.0",
    "source-map-explorer": "^2.5.2",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "~4.0.2"
  }
}
